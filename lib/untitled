<script type="text/javascript">

    function v2GetSignUpCommonModel() {
        var commonData = {};
        commonData.CommunityHubGUID = $('#CommunityHubGUID').val();
        commonData.OrganizationGUID = $('#OrganizationGUID').val();
        commonData.SignUpFormId = $('#SignUpFormId').val();
        commonData.CommunityHubName = $('#CommunityHubName').val();
        commonData.OrganizationName = $('#OrganizationName').val();
        commonData.activityGUID = $('#activityGUID').val();
        commonData.IsNewCommunityHubMember = $('#IsNewCommunityHubMember').val();

        return JSON.stringify(commonData);
    }

</script><noscript></noscript>
</div>
<div id="DescriptionHolder"  aria-live="assertive " ></div>

    <script type="text/javascript">
        $(function () {

            //this check is different from the jquery validator built in date check... it uses date.parse.
            $.validator.addMethod("IsValidDate", function (value, element) {
                return this.optional(element) || v2IsValidDate(value);
            });

            $("a.CustomFieldDescriptionButton").click(function (e) {
                e.preventDefault();
                var target = $(this).siblings('.InteractiveToolTip:first');
                $('#DescriptionHolder').html(target.html());

            });

            v2WireUpAccessibleToolTips();
            $('#SaveCustomFieldsButton').button();
            $('#CustomFieldsForm').validate();

            $('input.overMe').NewlabelOver();
            v2SetUpDatePicker($('#CustomFieldFormContainer').find('.DateTextBox'));

            $('.RequiredCF').each(function () {

                //add the required validator to these!
                $(this).rules('add', {
                    required: true,
                    messages: {
                        required: $(this).attr('requiredmessage')
                    }

                });

            });


            $('#CustomFieldFormContainer').find('input.DateTextBox').each(function () {

                $(this).rules('add', {
                    IsValidDate: true,
                    messages: {
                        IsValidDate: $(this).attr('validationmessage')
                    }

                });

            });

            //add the validation to the custom field number boxes
            $('#CustomFieldFormContainer').find('input.NumberTextBox').each(function () {
                $(this).rules('add', {
                    number: true,
                    messages: {
                        number: $(this).attr('validationmessage')
                    }

                });

            });

            $('input:checkbox.CustomFieldFileRemove').click(function () {
                var thebox = $(this);

                var fileSelector = $('#' + $(this).attr('fileSelector'));
                if (thebox.is(':checked')) {
                    fileSelector.val("");
                }
            });

            $("#SecondSaveButton").button().click(function () {
                $("#SaveCustomFieldsButton").trigger('click');
            });

            $('#SaveCustomFieldsButton').die();
            $('#SaveCustomFieldsButton').live('click', function (e) {
                e.preventDefault();
                var url = $('#UrlToSaveCustomFields').val();

                var form = $('#CustomFieldsForm');
                $('#ValidationErrorMessages').empty();
                if (form.valid()) {

                    var dataToPost = v2GetCustomFieldData();

                    //loop through and get all the files, put them in the fake form,  post that form using the jquery.forms plugin.
                    var fileControls = $('#CustomFieldFormContainer').find('input:file.CustomFieldFile');


                    $('#TempFormHolder').empty().append($('<form id="TempForm" name="TempForm" action="' + url + '"  method="post" enctype="multipart/form-data" />'));

                    var theForm = $('#TempForm');

                    var commonModel = v2GetSignUpCommonModel();

                    theForm.append($("<input type='text' id='model' name='model' />").val(dataToPost));
                    theForm.append($("<input type='text' id='commonModel' name='commonModel' />").val(commonModel));



                    fileControls.each(function () {
                        //NOTE: Can't Clone It: IE considers i ta security threat to set the value of a file element. makes sense actually.
                        $(this).hide();
                        var theClone = $(this).clone().insertAfter($(this));
                        theForm.append($(this));
                        theClone.show();
                    });


                    theForm.submit();

                }
                else {

                    v2ShowValidationErrors();
                }
            });

        });


        function v2GetCustomFieldData() {
            /*This the class the list items are being put into
            Public Class EditCustomFieldsPostListItem
            Public Property CustomFieldId As Integer
            Public Property CustomFieldTypeId As Integer
            Public Property CustomFieldValue As Object

            End Class
            */

            var holder = $('#CustomFieldFormContainer');
            var data = {};


            var customFields = new Array();

            //numbers...
            var numberBoxes = holder.find('input.CustomFieldNumberTextBox');

            numberBoxes.each(function () {
                var currentBox = $(this);
                var customFieldId = currentBox.attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.NumberId;
                var customFieldValue = currentBox.val();

                customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": customFieldValue });
            });

            //regular text
            var textBoxes = holder.find('input.CustomFieldTextTextBox');
            textBoxes.each(function () {
                var currentBox = $(this);
                var customFieldId = currentBox.attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.TextId;
                var customFieldValue = currentBox.val();

                customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": customFieldValue });
            });

            //big text
            var bigTextAreas = holder.find('textarea.CustomFieldBigText');
            bigTextAreas.each(function () {

                var currentBox = $(this);
                var customFieldId = currentBox.attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.BigTextId;
                var customFieldValue = currentBox.val();

                customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": customFieldValue });
            });

            //DateTime
            var dateTextBoxes = holder.find('input.DateTextBox[id^=CustomField]');
            dateTextBoxes.each(function () {
                var currentBox = $(this);
                var customFieldId = currentBox.attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.DateTimeId;
                var customFieldValue = currentBox.val();

                customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": customFieldValue });
            });

            //YES  (checkbox)
            var yesBoxes = holder.find('input.CustomFieldYes:enabled');
            yesBoxes.each(function () {
                var currentBox = $(this);
                var customFieldId = currentBox.attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.YesId;
                var customFieldValue = currentBox.is(':checked');

                customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": customFieldValue });
            });

            //YESNO
            var yesNoSelects = holder.find('select.YesNoSelect');
            yesNoSelects.each(function () {
                var currentBox = $(this);
                var customFieldId = currentBox.attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.YesNoId;
                var customFieldValue = currentBox.val();

                customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": customFieldValue });
            });

            //Custom Options
            var optionsSelects = holder.find('select.CustomFieldOptionSelect');
            optionsSelects.each(function () {
                var currentBox = $(this);
                var customFieldId = currentBox.attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.CustomFieldOptionId;
                var customFieldValue = currentBox.val();

                customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": customFieldValue });
            });

            //FILES
            var files = holder.find('input.CustomFieldFile');
            files.each(function () {

                var customFieldId = $(this).attr('customFieldId');
                var customFieldTypeId = CustomFieldTypes.FileId;
                var fileControl = $(this);
                var removeIt = $('#CustomFieldFileRemove' + customFieldId + ':checked').length > 0;
                if (removeIt) {
                    customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": null });

                }
                else {
                    if (fileControl.val() != "") {
                        //they picked a file, add it's info
                        customFields.push({ "CustomFieldId": customFieldId, "CustomFieldTypeId": customFieldTypeId, "CustomFieldValue": fileControl.val() });
                    }
                }

            });

            data.CustomFields = customFields;

            return JSON.stringify(data);



        }
    </script><noscript></noscript>


    </div>

