        function v2SetUpQualificationsForm() {
            $('#SaveQualificationsButton').button();
            $('#QualificationsForm').validate();
            $('#QualificationFormContainer').find('input.ExpiryDateBox').each(function () {

                $(this).rules('add', {
                    ExpiryDateIsRequiredIfLevelIsSelected: true,
                    messages: {
                        ExpiryDateIsRequiredIfLevelIsSelected: $(this).attr('ErrorMessage')
                    }

                });
            });

            $('input.overMe').NewlabelOver();

            var currentYear = new Date().getFullYear();
            var yearRange = "{0}:{1}".format(currentYear-10, currentYear + 11);

            v2SetUpDatePicker($('#QualificationFormContainer').find('.DateTextBox'), yearRange);

            $("a.InteractiveToolTipMe").click(function (e) {
                e.preventDefault();
                var target = $(this).siblings('.InteractiveToolTip:first');
                $('#DescriptionHolder').html(target.html());

            });


        }

        function v2BuildQualificationSaveData() {
            var data = {};

            var qualifications = new Array();
            $('select.QualificationSelect:not(:disabled)').each(function () {
                var currentSelect = $(this);
                var qualificationAreaId = currentSelect.siblings('input.QualificationAreaIdHidden').val();
                var qualificationLevelId = currentSelect.val();
                var currentQualificationData = { QualificationAreaId: qualificationAreaId, QualificationLevelId: qualificationLevelId };
                if ($('#ExpiryBoxFor' + qualificationAreaId).length > 0) {
                    currentQualificationData.ExpiryDate = $('#ExpiryBoxFor' + qualificationAreaId).val();
                }
                qualifications.push(currentQualificationData);
            });

            data.Qualifications = qualifications;


            return JSON.stringify(data);


        }


